{
  "name": "logic-bind-model",
  "version": "1.0.30",
  "homepage": "https://bindmodel.com/",
  "description": "white framework web",
  "type": "module",
  "main": "index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./types/index.d.ts",
        "default": "./types/index.d.ts"
      },
      "browser": {
        "require": "./dist/bind-model.browser.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/bind-model.node.cjs",
        "default": "./index.js"
      }
    },
    "./ko": {
      "types": {
        "require": "./types/ko/index.d.ts",
        "default": "./types/ko/index.d.ts"
      },
      "browser": {
        "require": "./dist/bind-model.browser.cjs",
        "default": "./index.js"
      },
      "default": {
        "require": "./dist/bind-model.node.cjs",
        "default": "./index.js"
      }
    }
  },
  "files": [
    "dist/",
    "types/",
    "src/",
    "index.js"
  ],
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:c": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:u": "node --experimental-vm-modules node_modules/.bin/jest --updateSnapshot",
    "test:v": "node --experimental-vm-modules node_modules/.bin/jest --verbose",
    "http": "npx http-server --cors",
    "type:en": "npx typedoc --out docs/en types/index.d.ts types/T.d.ts",
    "type:ko": "npx typedoc --out docs/ko types/ko/index.d.ts types/ko/T.d.ts",
    "build": "rollup -c --bundleConfigAsCjs",
    "check:ts": "tsc --noEmit",
    "check:js": "eslint src/ --ext .js",
    "check:all": "npm run check:ts && npm run check:js",
    "prepare": "npm run build",
    "update": "npm update && npm run build"
  },
  "author": "logicfeel",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/white-base/bind-model.git",
    "directory": "packages/react"
  },
  "bugs": "https://github.com/white-base/bind-model/issues",
  "keywords": [
    "BindModel",
    "BindCommand",
    "mvc",
    "controller",
    "DataSet",
    "DataTable",
    "DataView",
    "MetaSet",
    "MetaTable",
    "MetaView",
    "vue",
    "Vuex State",
    "react",
    "react state",
    "Redux, Context API, Recoil"
  ],
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "axios": "^1.8.4",
    "babel-jest": "^29.7.0",
    "broadcast-channel": "^7.1.0",
    "eslint": "^9.21.0",
    "fs": "^0.0.1-security",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdom": "^24.0.0",
    "msw": "^2.7.3",
    "path": "^0.12.7",
    "rollup": "^4.34.8",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-cleandir": "^3.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "stringify-parse": "^1.0.6",
    "telejson": "^7.2.0",
    "typedoc": "^0.25.12",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "jquery": "^3.6.4",
    "logic-entity": "github:white-base/meta-entity#dev"
  }
}
